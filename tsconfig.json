{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "amd",
    "noImplicitAny": false,
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "target": "ES2016",
    "lib": ["es2017", "dom", "esnext.array", "es2018.promise"],
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "./*": ["./*"]
    },
    "types": ["google.visualization"],
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": ["./src/**/*.ts", "./test/**/*.ts", "./types/**/*.d.ts"],
  "exclude": ["@types/**/*"],
  "atom": {
    "rewriteTsconfig": false
  }
}
